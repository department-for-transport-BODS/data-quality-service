{
  "QueryLanguage": "JSONata",
  "Comment": "Data Quality Service State Machine",
  "StartAt": "InitialProcessInput",
  "States": {
    "InitialProcessInput": {
      "Type": "Pass",
      "Next": "Initiate DQS Lambda",
      "Assign": {
        "revision_id": "{% $state.input %}" }
    },
    "Initiate DQS Lambda": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Output": "{% $states.result.Payload %}",
      "Arguments": {
        "FunctionName": "${InitiateDQSLambdaArn}",
        "Payload": "{% $revision_id %}"
      },
      "Retry": [
        {
          "ErrorEquals": [
            "Lambda.ServiceException",
            "Lambda.AWSLambdaException",
            "Lambda.SdkClientException",
            "Lambda.TooManyRequestsException"
          ],
          "IntervalSeconds": 1,
          "MaxAttempts": 3,
          "BackoffRate": 2,
          "JitterStrategy": "FULL"
        }
      ],
      "Next": "Map",
      "Catch": [
        {
          "ErrorEquals": [
            "States.ALL"
          ],
          "Next": "ParentExceptionHandler"
        }
      ]
    },
    "ParentExceptionHandler": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Output": "{% $states.result.Payload %}",
      "Arguments": {
        "FunctionName": "${GeneralErrorLambdaArn}",
        "Payload": "{% $states.input %}",
        "revision_id": "{% $revision_id %}"
      },
      "Retry": [
        {
          "ErrorEquals": [
            "Lambda.ServiceException",
            "Lambda.AWSLambdaException",
            "Lambda.SdkClientException",
            "Lambda.TooManyRequestsException"
          ],
          "IntervalSeconds": 1,
          "MaxAttempts": 3,
          "BackoffRate": 2,
          "JitterStrategy": "FULL"
        }
      ],
      "End": true
    },
    "Map": {
      "Type": "Map",
      "Label": "Map",
      "MaxConcurrency": 10,
      "Next": "Pipeline Monitor Lambda",
      "ItemSelector":{
        "revision_id": "{% $revision_id %}",
        "DQSCheck_MissingBusWorkingNumber": "${DQSCheck_MissingBusWorkingNumber}",
        "DQSCheck_MissingStop": "${DQSCheck_MissingStop}",
        "DQSCheck_IncorrectLicenceNumber": "${DQSCheck_IncorrectLicenceNumber}",
        "DQSCheck_IncorrectNoc": "${DQSCheck_IncorrectNoc}",
        "DQSCheck_IncorrectStopType": "${DQSCheck_IncorrectStopType}",
        "DQSCheck_FirstStopIsSetDownOnly": "${DQSCheck_FirstStopIsSetDownOnly}",
        "DQSCheck_LastStopIsPickUpOnly": "${DQSCheck_LastStopIsPickUpOnly}",
        "DQSCheck_FirstStopIsNotATimingPoint": "${DQSCheck_FirstStopIsNotATimingPoint}",
        "DQSCheck_LastStopIsNotATimingPoint": "${DQSCheck_LastStopIsNotATimingPoint}",
        "DQSCheck_DuplicateJourneyCode": "${DQSCheck_DuplicateJourneyCode}",
        "DQSCheck_StopNotFoundInNaptan": "${DQSCheck_StopNotFoundInNaptan}",
        "DQSCheck_SameStopFoundMultipleTimes": "${DQSCheck_SameStopFoundMultipleTimes}",
        "DQSCheck_NoTimingPointMoreThan15Mins": "${DQSCheck_NoTimingPointMoreThan15Mins}",
        "DQSCheck_MissingJourneyCode": "${DQSCheck_MissingJourneyCode}",
        "DQSCheck_CancelledServiceIncorrectlyAppearingActive": "${DQSCheck_CancelledServiceIncorrectlyAppearingActive}",
        "DQSCheck_ServicedOrganisationDataIsOutOfDate": "${DQSCheck_ServicedOrganisationDataIsOutOfDate}",
        "DQSCheck_DataPreFetch": "${DQSCheck_DataPreFetch}"
      },
      "Catch": [
        {
          "ErrorEquals": [
            "States.ALL"
          ],
          "Next": "ParentExceptionHandler"
        }
      ],
      "Output": {
        "mapResults": "{% $states.result %}"
      },
      "ResultWriter": {
        "Resource": "arn:aws:states:::s3:putObject",
        "Arguments": {
          "Bucket": "${S3BucketName}",
          "Prefix": "dqs-map-results"
        }
      },
